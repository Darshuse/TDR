                    <xs:documentation source="Name" xml:lang="EN">FinancialInstrumentIdentification</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Securities for which the meeting is organised.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstrTpSts" type="InstructionTypeStatus3Choice">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">InstructionTypeStatus</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Type of instruction status.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CnfrmgPty" type="PartyIdentification226Choice">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">ConfirmingParty</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Party that confirms the receipt of the vote cast.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="VoteCstgPty" type="PartyIdentification226Choice">
