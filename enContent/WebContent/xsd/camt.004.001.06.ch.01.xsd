      <xs:element name="Dt" type="ISODate"/>
      <xs:element name="DtTm" type="ISODateTime"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="DateAndDateTimeChoice_CH_camt004">
    
        <xs:choice>
          <xs:element name="Dt" type="ISODate">
            <xs:annotation>
              <xs:documentation source="Name" xml:lang="EN">Settlement Date</xs:documentation>
              <xs:documentation source="Name" xml:lang="DE">Verrechnungsdatum</xs:documentation>
              <xs:documentation xml:lang="DE" source="Definition">Es werden alle verfügbaren Salden des abgefragten Verrechnungsdatums geliefert.</xs:documentation>
              <xs:documentation xml:lang="EN" source="Definition">All available balances for the specified date will be provided.</xs:documentation>
            </xs:annotation>
          </xs:element>
