            <xs:annotation>
              <xs:documentation source="Name" xml:lang="EN">Reference Type</xs:documentation>
              <xs:documentation source="Name" xml:lang="DE">Referenztyp</xs:documentation>
              <xs:documentation xml:lang="DE" source="Definition"/>
