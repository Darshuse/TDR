        <xs:sequence>
          <xs:element name="Cd" type="Instruction4Code" minOccurs="0">
            <xs:annotation>
              <xs:documentation source="Name" xml:lang="EN">Processing Instruction (code)</xs:documentation>
              <xs:documentation source="Name" xml:lang="DE">Verarbeitungsinstruktionen (Code)</xs:documentation>
              <xs:documentation xml:lang="DE" source="Definition">Element darf nicht verwendet werden.</xs:documentation>
